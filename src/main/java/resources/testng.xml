<?xml version="1.0" encoding="UTF-8"?>
<suite name="DemoblazeSuite" verbose="1" parallel="false" thread-count="1">
    <parameter name="browser" value="chrome"/>
    
    <listeners>
        <listener class-name="listeners.TestListener"/>
    </listeners>
    
    <test name="ProductPurchaseTests" preserve-order="true">
        <classes>
            <class name="tests.ProductPurchaseTest">
                <methods>
                    <include name="testCompleteProductPurchaseFlow"/>
                    <include name="testAddProductToCart"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>